#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/akshxdevs/codes/projects/web3/ecom-dapp/sdk/node_modules/.pnpm/openai@4.104.0_ws@7.5.10_bufferutil@4.0.9_utf-8-validate@5.0.10_/node_modules/openai/bin/node_modules:/home/akshxdevs/codes/projects/web3/ecom-dapp/sdk/node_modules/.pnpm/openai@4.104.0_ws@7.5.10_bufferutil@4.0.9_utf-8-validate@5.0.10_/node_modules/openai/node_modules:/home/akshxdevs/codes/projects/web3/ecom-dapp/sdk/node_modules/.pnpm/openai@4.104.0_ws@7.5.10_bufferutil@4.0.9_utf-8-validate@5.0.10_/node_modules:/home/akshxdevs/codes/projects/web3/ecom-dapp/sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/akshxdevs/codes/projects/web3/ecom-dapp/sdk/node_modules/.pnpm/openai@4.104.0_ws@7.5.10_bufferutil@4.0.9_utf-8-validate@5.0.10_/node_modules/openai/bin/node_modules:/home/akshxdevs/codes/projects/web3/ecom-dapp/sdk/node_modules/.pnpm/openai@4.104.0_ws@7.5.10_bufferutil@4.0.9_utf-8-validate@5.0.10_/node_modules/openai/node_modules:/home/akshxdevs/codes/projects/web3/ecom-dapp/sdk/node_modules/.pnpm/openai@4.104.0_ws@7.5.10_bufferutil@4.0.9_utf-8-validate@5.0.10_/node_modules:/home/akshxdevs/codes/projects/web3/ecom-dapp/sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli" "$@"
else
  exec node  "$basedir/../../bin/cli" "$@"
fi
